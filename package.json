  "name": "react-playground",
  "private": true,
  "scripts": {
    "build": "tsc -b && vite build",
    "check": "pnpm i && echo install success && pnpm check:tsc && pnpm check:biome && pnpm check:knip && pnpm build && pnpm check:eslint && echo check success",
    "check:biome": "biome check --error-on-warnings --write --unsafe src && echo check:biome success",
    "check:eslint": "eslint . && echo check:eslint success",
    "check:knip": "knip && echo check:knip success",
    "check:tsc": "tsc --noEmit && echo check:tsc success",
    "dev": "vite",
    "preview": "vite preview"
  },
